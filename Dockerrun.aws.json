{
    "AWSEBDockerrunVersion" : 2,
    "containerDefinitions" : [
        {
            "name": "fib-client",
            "image": "deshneil/fib-client",
            "hostname": "fib-client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "server",
            "image": "deshneil/fib-server",
            "hostname": "api",
            "essential": false,
            "memory": 128
        },
        {
            "name": "worker",
            "image": "deshneil/fib-worker",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "deshneil/fib-nginx",
            "essential": true,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["fib-client","server"]
        }
    ]
}